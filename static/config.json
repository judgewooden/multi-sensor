    "Logging":{
        "version":1,
        "disable_existing_loggers":true,
        "formatters":{
            "brief":{
                "class":"logging.Formatter",
                "datefmt":"%I:%M:%S",
                "format":"%(name)s[%(process)d]: (%(codifier)s) %(levelname)s %(message)s"
            },
            "verbose":{
                "class":"logging.Formatter",
                "datefmt":"%I:%M:%S",
                "format":"%(name)s %(codifier)s %(levelname)s (%(lineno)d) %(message)s"
            }
        },
        "handlers":{
            "operator":{
                "level":"ERROR",
                "class":"logging.StreamHandler",
                "formatter":"verbose",
                "stream":"ext://sys.stdout"
            },
            "console":{
                "level":"INFO",
                "class":"logging.StreamHandler",
                "formatter":"verbose",
                "stream":"ext://sys.stdout"
            },
            "syslog":{
                "level":"INFO",
                "class":"logging.handlers.SysLogHandler",
                "facility":"daemon",
                "formatter":"brief",
                "address":"/dev/log"
            }
        },
        "loggers":{
            "jphMonitor": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            },
            "jphRedis": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            },
            "jphPostgreSQL": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            }
        },
        "root": {
            "handlers":["operator"],
            "level":"NOTSET"
        }

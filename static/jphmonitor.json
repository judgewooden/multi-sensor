
{
    "Multicast": {
        "Control-Channel": {
            "Address":"225.0.0.251",
            "Port":8001
        },
        "Data-Channel": {
            "Address":"225.0.0.251",
            "Port":8002
        }
    },
    "Logging":{
        "version":1,
        "disable_existing_loggers":true,
        "formatters":{
            "brief":{
                "class":"logging.Formatter",
                "datefmt":"%I:%M:%S",
                "format":"%(name)s[%(process)d]: %(levelname)s (%(lineno)d) %(message)s"
            },
            "verbose":{
                "class":"logging.Formatter",
                "datefmt":"%I:%M:%S",
                "format":"%(levelname)s (%(lineno)d) %(message)s"
            }
        },
        "handlers":{
            "console":{
                "level":"WARN",
                "class":"logging.StreamHandler",
                "formatter":"verbose",
                "stream":"ext://sys.stdout"
            },
            "syslog":{
                "level":"INFO",
                "class":"logging.handlers.SysLogHandler",
                "facility":"daemon",
                "formatter":"brief",
                "address":"/dev/log"
            }
        },
        "loggers":{
            "default": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            },
            "mRedis": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            },
            "mSQL": {
                "handlers":["syslog", "console"],
                "level":"INFO"
            }
        },
        "root": {
            "handlers":["console"],
            "level":"WARN"
        }
    },
    "Sensors":[{
            "Codifier":"CC",
            "ShortName":"Control",
            "Name":"Control, show number of message on Multicast Bus or latest Console message",
            "Symbol":"p/m",
            "KeepAliveInterval":60,
            "SensorInterval":60,
            "Active":false,
            "DataType":"integer",
            "Sensor":{
                "Type":"ControlProgram"
            }
        }, {
            "Codifier":"Q1",
            "ShortName":"SQL-jphpi",
            "Name":"Postgress SQL adaptor on jphpi",
            "Symbol":"p/m",
            "KeepAliveInterval":60,
            "SensorInterval":60,
            "Active":true,
            "DataType":"integer",
            "Sensor":{
                "Type":"SQLLoader",
                "SQL":{
                    "Database":"mysensor6",
                    "User":"jphadmin",
                    "Password":"~/.sqlpassword",
                    "Host":"127.0.0.1"
                }
            }
        }, {
            "Codifier":"R1",
            "ShortName":"Redis-mad",
            "Name":"Redis-Cache on mad",
            "Symbol":"p/m",
            "KeepAliveInterval":60,
            "SensorInterval":60,
            "Active":true,
            "DataType":"integer",
            "Sensor":{
                "Type":"RedisLoader"
            }
        }, {
